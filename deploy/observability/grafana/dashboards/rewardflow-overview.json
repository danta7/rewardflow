  "panels": [
    {
      "type": "timeseries",
      "title": "PlayReport QPS (by scene)",
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "ops"
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "sum by (scene) (rate(rewardflow_play_report_total[1m]))",
          "legendFormat": "{{scene}}",
          "refId": "A"
        }
      ],
      "gridPos": {
        "h": 9,
        "w": 12,
        "x": 0,
        "y": 0
      }
    },
    {
      "type": "timeseries",
      "title": "PlayReport Errors (by result)",
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "ops"
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "sum by (result) (rate(rewardflow_play_report_total{result=~\"biz_error|error\"}[1m]))",
          "legendFormat": "{{result}}",
          "refId": "A"
        }
      ],
      "gridPos": {
        "h": 9,
        "w": 12,
        "x": 12,
        "y": 0
      }
    },
    {
      "type": "timeseries",
      "title": "HTTP Server P95 (seconds)",
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (le) (rate(http_server_requests_seconds_bucket{uri=~\"/api/v1/.*\"}[5m])))",
          "legendFormat": "p95",
          "refId": "A"
        }
      ],
      "gridPos": {
        "h": 9,
        "w": 12,
        "x": 0,
        "y": 9
      }
    },
    {
      "type": "timeseries",
      "title": "Outbox Published / min (by event_type)",
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "60 * sum by (event_type) (rate(rewardflow_outbox_publish_total{result=\"SENT\"}[1m]))",
          "legendFormat": "{{event_type}}",
          "refId": "A"
        }
      ],
      "gridPos": {
        "h": 9,
        "w": 12,
        "x": 12,
        "y": 9
      }
    },
    {
      "type": "stat",
      "title": "Outbox Pending (count)",
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "targets": [
        {
          "expr": "rewardflow_outbox_pending",
          "refId": "A"
        }
      ],
      "gridPos": {
        "h": 6,
        "w": 6,
        "x": 0,
        "y": 18
      }
    },
    {
      "type": "timeseries",
      "title": "Award Issue Rate (by prize_code)",
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "ops"
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "sum by (prize_code) (rate(rewardflow_award_issue_total[1m]))",
          "legendFormat": "{{prize_code}}",
          "refId": "A"
        }
      ],
      "gridPos": {
        "h": 6,
        "w": 18,
        "x": 6,
        "y": 18
      }
